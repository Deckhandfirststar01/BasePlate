{
    "name": "BasePlate",
    "description": "A modern WordPress stack. Don't repeat yourself.",
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
          "type": "package",
          "package": {
            "name": "advanced-custom-fields/advanced-custom-fields-pro",
            "version": "5.5.14",
            "type": "wordpress-plugin",
            "dist": {
              "type": "zip",
              "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9OTAwNTh8dHlwZT1kZXZlbG9wZXJ8ZGF0ZT0yMDE2LTA5LTIwIDE1OjA2OjEy"
            }
          }
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "wpackagist-plugin/wordpress-seo": "^4.8",
        "wpackagist-plugin/custom-post-type-ui": "^1.5.3",
        "advanced-custom-fields/advanced-custom-fields-pro": "^5.5.14",
        "wpackagist-plugin/support-hours": "^1.3.1",
        "wpackagist-plugin/trigger-browsersync": "^0.5"
