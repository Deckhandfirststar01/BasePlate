{
    "name": "BasePlate",
    "description": "Great theme for small-medium sized projects for oldschool FTP deploment, featuring top-notch build tools.",
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      }
    ],
    "require": {
        "wpackagist-plugin/wordpress-seo": "^4.9",
        "wpackagist-plugin/custom-post-type-ui": "^1.5.7",
        "wpackagist-plugin/support-hours": "^1.4.2"
    },
    "extra": {
        "installer-paths": {
            "../../plugins/{$name}": ["type:wordpress-plugin"]
        }
    }
}
